{
  "name": "cidr-visualizer",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "bundle": "tar -cvf ./deploy.tar --exclude='*.map' ./captain-definition ./dist/*",
    "deploy": "pnpm run build && pnpm run bundle && pnpx caprover deploy -t ./deploy.tar --default",
    "dev": "vite",
    "format": "oxfmt .",
    "lint": "oxlint --type-aware .",
    "lint:fix": "oxlint --fix --type-aware .",
    "preview": "vite preview",
    "test": "vitest run",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "dependencies": {
    "@ariakit/react": "^0.4.21",
    "@plausible-analytics/tracker": "^0.4.4",
    "@tailwindcss/vite": "^4.1.18",
    "clsx": "^2.1.1",
    "immer": "^11.1.3",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "oxlint-tsgolint": "^0.11.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "^4.0.17"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
